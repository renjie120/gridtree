<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
	<data-sources />
	<form-beans />
	<global-exceptions />
	<global-forwards />
	<action-mappings>
		<action path="/common" parameter="method" scope="request"
			type="testGridTree.action.TableTreeAction">
		</action>
		<action path="/lazy" parameter="method" scope="request"
			type="testGridTree.action.TableTreeLazyAction">
		</action>
		<action path="/initlazy" parameter="method" scope="request"
			type="testGridTree.action.InitTableTreeLazyAction">
		</action>
		<action path="/lazyPaging" parameter="method" scope="request"
			type="testGridTree.action.TableTreeLazyPagingAction">
		</action>
	</action-mappings>
	<message-resources
		parameter="com.yourcompany.struts.ApplicationResources" />
</struts-config>

